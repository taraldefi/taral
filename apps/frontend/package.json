{
  "name": "taral-frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev -p 4200",
    "start-mock-server": "ts-node --project scripts/tsconfig.scripts.json scripts/mock-server.ts",
    "build": "next build",
    "start": "next start -p 4200",
    "lint": "next lint",
    "check-types": "tsc --pretty",
    "check-format": "prettier --check .",
    "format": "prettier --write .",
    "check-lint": "eslint . --ext ts --ext tsx --ext js",
    "test": "jest --watch --verbose --silent",
    "test-all": "ts-node --project scripts/tsconfig.scripts.json scripts/run-checks.ts",
    "reinstall-ui-lib": "yarn remove taral-ui && yarn add taral-ui@latest"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^6.1.2",
    "@fortawesome/free-solid-svg-icons": "^6.1.1",
    "@fortawesome/react-fontawesome": "^0.2.0",
    "@micro-stacks/react": "^1.0.9",
    "@react-pdf-viewer/core": "^3.6.0",
    "@react-pdf-viewer/default-layout": "^3.6.0",
    "@react-pdf-viewer/thumbnail": "^3.6.0",
    "@react-pdf-viewer/zoom": "^3.6.0",
    "@stacks/connect": "^7.3.1",
    "@tanstack/query-core": "^5.0.5",
    "@tanstack/react-query": "^5.0.5",
    "axios": "^1.5.1",
    "cors": "^2.8.5",
    "express": "^4.18.1",
    "jotai": "^2.0.4",
    "jotai-query-toolkit": "^0.1.18",
    "jotai-tanstack-query": "^0.7.2",
    "next": "13.5.0",
    "nextjs-progressbar": "^0.0.16",
    "pdfjs-dist": "^3.11.174",
    "react": "18.2.0",
    "react-content-loader": "^6.2.1",
    "react-dom": "18.2.0",
    "react-feather": "^2.0.10",
    "react-hook-form": "^7.33.1",
    "react-icons": "^4.11.0",
    "react-pdf-thumbnail": "^0.1.0",
    "react-pro-sidebar": "^1.0.0",
    "react-router-dom": "^6.3.0",
    "react-toastify": "^9.1.3",
    "sass": "^1.52.2"
  },
  "devDependencies": {
    "@commitlint/cli": "^18.2.0",
    "@commitlint/config-conventional": "^17.6.6",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@types/cors": "^2.8.14",
    "@types/express": "^4.17.20",
    "@types/jest": "^29.5.1",
    "@types/next": "^9.0.0",
    "@types/node": "^17.0.42",
    "@types/react": "^18.2.16",
    "@types/react-dom": "^18.0.6",
    "@types/styled-components": "^5.1.28",
    "eslint": "8.52.0",
    "eslint-config-next": "14.0.2",
    "eslint-plugin-prettier": "5.0.0",
    "husky": "^8.0.0",
    "jest": "^29.5.0",
    "jest-environment-jsdom": "^29.7.0",
    "prettier": "^2.7.1",
    "typescript": "^4.7.3"
  }
}
